{
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "objectWrap": "preserve",
    "printWidth": 300,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "plugins": [
      "prettier-plugin-organize-attributes",
      "prettier-plugin-tailwindcss"
    ],
    "attributeGroups": [
      "^(id|name)$",
      "^type$",
      "^class$",
      "^data-.*$",
      "^(src|for|href|value)$",
      "^(title|alt)$",
      "^(role|aria-.*)$",
      "^tabindex$",
      "^style$",
      "$DEFAULT"
  ],
  "attributeSort": "ASC",
  "attributeIgnoreCase": true
}